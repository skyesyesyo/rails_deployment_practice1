<h1>Users#new</h1>
<p>Find me in app/views/users/new.html.erb</p>

<form action="/users" method="POST">
	<p><input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>"></p>
	<p>Email: <input type="text" id="Email" name="user[email]"></p>
	<p>Name: <input type="text" id="Name" name="user[name]"></p>
	<p>Password: <input type="text" id="Password" name="user[password]"></p>
	<p>Password Confirmation: <input type="text" id="Password_Confirmation" name="user[password_confirmation"></p>
	<p><input type="submit" name="Join" value="Join"></p>
</form>